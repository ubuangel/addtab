{
  "manifest_version": 2,
  "name": "Añadir a Grupo de Pestañas",
  "version": "1.0",
  "description": "Permite añadir pestañas al grupo existente desde el menú contextual",
  "permissions": [
    "tabs",
    "tabHide",
    "contextMenus",
    "storage",
    "notifications",
    "activeTab",
    "windows"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icons/icon-48.svg",
    "96": "icons/icon-96.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "add-to-tab-group@example.com",
      "strict_min_version": "89.0"
    }
  }
}